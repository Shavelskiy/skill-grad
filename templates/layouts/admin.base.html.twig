<!DOCTYPE html>
<html lang="ru">
<head>
  <meta charset="UTF-8">
  <title>{% block title %}Admin{% endblock %}</title>

  {% block stylesheets %}
    {{ encore_entry_link_tags('admin.layout') }}
  {% endblock %}
</head>
<body>
{% include 'admin/parts/sidebar.twig' %}

<div class="main-content">
  {% include 'admin/parts/navbar.twig' %}

  <div class="container-fluid px-4">
    {% block body %}{% endblock %}
  </div>
</div>

{% block javascripts %}
  {{ encore_entry_script_tags('admin.layout') }}
{% endblock %}
</body>
</html>
